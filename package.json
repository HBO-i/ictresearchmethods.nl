{
	"name": "ictmethods",
	"version": "1.0.1",
	"repository": {
		"type": "git",
		"url": "https://github.com/jochemvogel/ictmethods.nl.git"
	},
	"bugs": {
		"url": "https://github.com/jochemvogel/ictmethods.nl/issues/new?assignees=&labels=&template=bug_report.yml"
	},
	"license": "MIT",
	"scripts": {
		"dev": "vite dev",
		"dev-expose": "vite dev --host",
		"generate:uploads": "node ./scripts/test-build.js",
		"build": "vite build && generate:uploads",
		"preview": "vite preview",
		"preview-expose": "vite preview --host",
		"prepare-husky": "husky install",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint .",
		"format": "prettier --write --plugin-search-dir=. .",
		"test": "jest",
		"test:watch": "npm run test -- --watch"
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^1.0.0-next.42",
		"@sveltejs/kit": "next",
		"@testing-library/jest-dom": "^5.16.4",
		"@testing-library/svelte": "^3.1.3",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"eslint": "^8.16.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"husky": "^8.0.1",
		"jest": "^28.1.1",
		"jest-environment-jsdom": "^28.1.1",
		"mdsvex": "^0.10.6",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"sass": "^1.52.3",
		"svelte": "^3.49.0",
		"svelte-check": "^2.7.1",
		"svelte-focus-trap": "^1.2.0",
		"svelte-jester": "^2.3.2",
		"svelte-preprocess": "^4.10.7",
		"ts-jest": "^28.0.5",
		"tslib": "^2.3.1",
		"typescript": "^4.7.4"
	},
	"type": "module",
	"dependencies": {
		"fs": "^0.0.1-security",
		"fs-extra": "^11.1.1",
		"glob": "^10.2.1",
		"match-sorter": "^6.3.1",
		"path": "^0.12.7",
		"process": "^0.11.10",
		"sharp": "^0.32.0"
	}
}
